<div class="main">
  <div id="home" class="hidden">
    <%= render 'user/home/index/home' %>
  </div>
  <div id="benefit" class="hidden">
    <%= render 'user/home/index/benefit' %>
  </div>
  <div id="contact" class="hidden">
    <%= render 'user/home/index/contact' %>
  </div>
  <div id="mine" class="hidden">
    <%= render 'user/home/index/mine' %>
  </div>
</div>

<script>
    function set_tab() {
        let tab_id;
        <% unless params[:tab].blank? %>
        tab_id = '<%= params[:tab].strip %>';
        <% end %>
        if (!tab_id)
            tab_id = localStorage.getItem('mobile:home:tab');
        if (!tab_id)
            tab_id = TAB_IDS[0];
        change_main_tab(tab_id);
    }

    $(function () {
        set_tab();
    });
</script>